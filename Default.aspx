    <script type="text/javascript">

        var mySwiper = new Swiper(".swiper-container", {
            direction: "vertical",
            loop: true,
            pagination: ".swiper-pagination",
            grabCursor: true,
            speed: 5000,
            paginationClickable: true,
            parallax: true,
            autoplay: false,
            effect: "slide",
            mousewheelControl: 1
        });

        document.getElementById("swiper-container").addEventListener("wheel", ServiceTittleChange);



        function ServiceTittleChange() {
            var x = $(".swiper-pagination").find('.swiper-pagination-bullet-active').index();

            switch (x) {
                case 0:
                    $('.service_tittle span').text('Branding');
                    break;


                case 1:
                    $('.service_tittle span').text('Advertising');
                    break;


                case 2:
                    $('.service_tittle span').text('Strategy');
                    break;

                case 3:
                    $('.service_tittle span').text('Design');
                    break;

                case 4:
                    $('.service_tittle span').text('Social');
                    break;

                case 5:
                    $('.service_tittle span').text('Idea');
                    break;

            }
        }
    </script>


